package DBRPC.DBModel;

import java.util.List;

/**    
 *     
 * 项目名称：DBQuery    
 * 类名称：SQLModel    
 * 类描述：   参数化信息
 * 创建人：jinyu    
 * 创建时间：2018年3月18日 上午12:15:16    
 * 修改人：jinyu    
 * 修改时间：2018年3月18日 上午12:15:16    
 * 修改备注：    
 * @version     
 *     
 */
public class SQLModel {
    
    /**
     * SQL语句或者文件
     */
public String sql;

/**
 * 参数信息
 * 文件传输时文件内容
 */
public List<Object> list=null;
public String netType="json";//byte[]

/**
 * 多条SQL语句
 */
public String[] sqls;

/**
 * 批量时的数据量
 */
public int commitNum=1000;
public ResultCode returnCode=ResultCode.table;//xml,,data
}
